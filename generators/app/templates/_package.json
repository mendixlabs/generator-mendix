    "grunt-contrib-watch": "^1.0.0",
    "grunt-newer": "^1.2.0",
    "node-base64-image": "^0.1.2",
    "node-mendix-modeler-path": "https://github.com/JelteMX/node-mendix-modeler-path/archive/v1.0.0.tar.gz",
    "semver": "^5.1.0",
